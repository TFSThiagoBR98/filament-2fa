        "laravel",
        "filament",
        "2fa"
    ],
    "homepage": "https://github.com/tfsthiagobr98/filament-2fa",
    "license": "MIT",
    "authors": [
        {
            "name": "Danilo Andrade",
            "email": "danilo@webbingbrasil.com.br",
            "role": "Developer"
        },
        {
            "name": "Thiago Fran√ßa",
            "email": "contact@tfs.dev.br",
