        "ext-json": "*",
        "spatie/laravel-package-tools": "^1.19",
        "filament/filament": "^3.3",
        "laragear/two-factor": "^3.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "TFSThiagoBR98\\FilamentTwoFactor\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TFSThiagoBR98\\FilamentTwoFactor\\FilamentTwoFactorProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
